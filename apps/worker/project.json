{
  "name": "worker",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/worker/src",
  "projectType": "application",
  "targets": {
    "install": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/worker",
        "command": "python3 -m venv .venv && .venv/bin/python -m pip install -U pip && .venv/bin/python -m pip install -r requirements.txt"
      }
    },
    "run": {
      "executor": "nx:run-commands",
      "dependsOn": ["install"],
      "options": {
        "cwd": "apps/worker/src",
        "command": "../.venv/bin/python -m worker"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "dependsOn": ["install"],
      "options": {
        "cwd": "apps/worker/src",
        "command": "../.venv/bin/python -m worker"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": ["install"],
      "options": {
        "cwd": "apps/worker",
        "command": ".venv/bin/python -m compileall src"
      }
    }
  },
  "tags": []
}

